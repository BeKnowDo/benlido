    "browser-sync": "^2.26.7"
  },
  "testURL": "http://localhost",
  "dependencies": {
    "args": "^5.0.1",
    "autoprefixer": "^9.6.1",
    "chalk": "^2.4.2",
    "clean-css": "^4.2.1",
    "concurrently": "^4.1.1",
    "dotenv": "^8.0.0",
    "fs-extra": "^8.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "onchange": "^6.0.0",
    "postcss": "^7.0.17",
    "qa": "0.0.1"
  }
