{
  "name": "benlido",
  "version": "2.0.0",
  "description": "Ben-Lido Frontend Styling and JavaScript",
  "main": "index.js",
  "scripts": {
    "styles": "node scripts/styles",
    "styles:watch": "onchange 'src/styles/**/*.scss' -- npm run styles",
    "start": "nodemon ./scripts & npm run styles:watch",
    "dev": "concurrently \"npm run styles:watch\" \"npm run start\""
  },
