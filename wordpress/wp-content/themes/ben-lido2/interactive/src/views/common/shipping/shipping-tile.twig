<div class="shipping-tile">

  <img class="shipping-tile-image" src="{{shipping.image}}" alt=""/>
  <h2 class="shipping-tile-header">{{shipping.header}}</h2>
  <div class="shipping-tile-copy">{{shipping.copy}}</div>

  <button class="btn btn-block btn-lg btn-primary" name="frequency" value="{{shipping.number_of_days}}">Choose
    <i class="far fa-calendar"></i>
  </button>

</div>