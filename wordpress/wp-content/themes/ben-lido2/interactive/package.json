{
  "name": "benlido",
  "version": "1.0.0",
  "description": "Ben-Lido Frontend Templates",
  "main": "index.js",
  "scripts": {
    "json": "node scripts/seeder.js quiet=true",
    "server": "nodemon -e js,twig server",
    "api": "concurrently \"yarn server\" \"yarn json\"",
    "fed": "concurrently \"yarn fonts\" \"yarn styles\" \"yarn styles:watch\" \"yarn webpack\" \"yarn server\"",
    "wp-fed": "concurrently \"yarn fonts --wordpress\" \"yarn styles --wordpress\" \"yarn styles:watch --wordpress\" \"yarn webpack:dev --wordpress\" \"yarn twig:watch --wordpress\"",
    "wordpress": "concurrently \"yarn fonts --wordpress\" \"yarn styles --wordpress\" \"webpack --wordpress --config webpack.config.js --mode production\" \"yarn twig\"",
    "wordpress:watch": "onchange 'src/views/**/*.twig' 'src/styles/**/*.scss' 'src/javascript/**/*.js' -- yarn wordpress",
    "dev": "concurrently \"yarn fonts\" \"yarn styles\" \"yarn styles:watch\" \"yarn twig:watch\" \"yarn webpack\" \"yarn server\" \"yarn wordpress:watch\"",
    "beta": "concurrently \"yarn fonts\" \"yarn styles\" \"yarn styles:watch\" \"yarn twig:watch\" \"npm run webpack \" \"yarn server\" \"yarn wordpress:watch\"",
    "styles": "node scripts/styles.js",
    "styles:watch": "onchange 'src/styles/**/*.scss' -- yarn styles",
    "fonts": "node scripts/fonts.js",
    "images": "node scripts/images.js",
    "twig": "node scripts/twig.js",
    "twig:watch": "onchange 'src/views/**/*.twig' -- yarn twig",
    "webpack": "webpack --mode production --config webpack.config.js",
    "webpack:dev": "webpack --watch --mode development --config webpack.config.js",
    "webpack-beta": "webpack --wordpress--mode production --config webpack.beta.config.js",
    "webpack-beta-dev": "concurrently \"yarn fonts --wordpress\" \"yarn styles --wordpress\" \"yarn styles:watch --wordpress\" \"webpack --wordpress --mode development --config webpack.beta.config.js\" \"yarn twig:watch --wordpress\"",
    "puppet": "node puppeteer",
    "puppet-dev": "nodemon puppeteer"
  },
  "proxy": {},
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/bkd-digital/bdk-benlido.git"
  },
  "keywords": [
    "frontend",
    "wordpress"
  ],
  "author": "Cesar M. Perez",
  "license": "ISC",
  "homepage": "https://bitbucket.org/bkd-digital/bdk-benlido#readme",
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "args": "^5.0.0",
    "autoprefixer": "^8.6.5",
    "babel-loader": "^8.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "browser-sync": "^2.24.7",
    "chalk": "^2.4.1",
    "clean-css": "^4.2.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^2.0.0",
    "concurrently": "^3.6.1",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "fs-extra": "^7.0.0",
    "image-downloader": "^3.4.1",
    "imagemin": "^5.3.1",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.1.0",
    "inquirer": "^6.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "nocache": "^2.0.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "onchange": "^4.1.0",
    "opn": "^5.3.0",
    "postcss": "^6.0.23",
    "puppeteer": "^1.8.0",
    "qs": "^6.5.2",
    "standard": "^12.0.1",
    "twig": "^1.12.0",
    "webpack": "^4.19.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "yargs": "^11.1.0"
  },
  "testURL": "http://localhost",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "body-scroll-lock": "^2.5.7",
    "element-closest": "^2.0.2",
    "in-view": "^0.6.1",
    "in-viewport": "^3.6.0",
    "kute.js": "^1.6.5",
    "lodash": "^4.17.19",
    "mo-js": "^0.288.2",
    "notie": "^4.3.1",
    "promise": "^8.0.2",
    "selectivity": "^3.1.0",
    "spectre.css": "^0.5.3",
    "sweetalert": "^2.1.0",
    "swiper": "^4.4.1",
    "whatwg-fetch": "^2.0.4"
  }
}
